#dnl test if environment variable is defined
#ifdef ENV_VAR
ENV_VAR is defined as %{ENV_VAR}.
#endif
#dnl test if #define is defined
#define MYDEF 42
#ifdef MYDEF
MYDEF is defined as %{MYDEF}.
#endif
#dnl test if an undefined value is defined
#ifdef NOT_DEFINED
NOT_DEFINED is defined as %{NOT_DEFINED}.
#endif
